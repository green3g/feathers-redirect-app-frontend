{
  "name": "feathers-redirect-app-frontend",
  "version": "1.0.0",
  "description": "An example app for CanJS, using StealJS.",
  "author": "Bitovi",
  "license": "MIT",
  "homepage": "https://github.com/canjs/feathers-app-admin-gui.js#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canjs/feathers-app-admin-gui.js.git"
  },
  "bugs": {
    "url": "https://github.com/canjs/feathers-app-admin-gui.js/issues"
  },
  "keywords": [
    "CanJS",
    "DoneJS",
    "StealJS"
  ],
  "main": "src/main.js",
